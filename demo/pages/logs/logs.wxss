.seakgroup {
  width: 100%;
  height: 100rpx;
  background: green;
  display: inline-block;
  position: relative;
  z-index: 1;
}

.seakgroup .search {
  border: 1px solid #ccc;
  margin: 20rpx auto;
  width: 90%;
  border-radius: 4rpx;
  background: #fff;
}

.seakgroup .icon {
  position: absolute;
  top: 34rpx;
  left: 35%;
  z-index: 100;
}

.list {
  width: 90%;
  height: 110rpx;
  margin: 10px 5%;
  background: gray;
  display: inline-flex;
  font-size:32rpx;
}

.list .photo {
  width: 40rpx;
  height: 40rpx;
  margin-left: 10rpx;
  flex-grow: 1;
  margin-top: 12rpx;
  display:block;
}
.list .photo>imgage{
  width:100%;
  height:100%;
  display: inline-block;
}
.list .right {
  flex-grow: 3;
  text-align: left;
  line-height: 2;
  font-size: 16px;
}
